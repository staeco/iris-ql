{
  "name": "iris-ql",
  "version": "5.0.0",
  "description": "User friendly API query language",
  "main": "dist/index.js",
  "keywords": [
    "sequelize",
    "query language",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/iris-ql.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/iris-ql/issues"
  },
  "homepage": "https://github.com/staeco/iris-ql#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts --theme minimal && gh-pages -d docs",
    "lint": "prettier --write .",
    "build": "npm run clean && tsc -b",
    "clean": "rm -rf dist",
    "test-make-user": "createuser postgres -lsd",
    "test-db": "dropdb iris-ql -f -U postgres || true && createdb iris-ql -U postgres && psql iris-ql -c 'CREATE EXTENSION IF NOT EXISTS postgis;' -U postgres",
    "test": "npm run-script test-db && NODE_ENV=test nyc mocha --require ts-node/register --require source-map-support/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint",
    "test:ci": "NODE_ENV=test nyc mocha --require ts-node/register --require source-map-support/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": true
  },
  "peerDependencies": {
    "pg": ">=7",
    "sequelize": ">=4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/pg": "^7.14.11",
    "@types/pluralize": "^0.0.29",
    "get-stream": "^6.0.0",
    "husky": "^4.3.8",
    "jsonstream-next": "^3.0.0",
    "mocha": "^8.0.0",
    "nyc": "^15.1.0",
    "pg": "^8.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "pumpify": "^2.0.1",
    "rimraf": "^3.0.0",
    "sequelize": "^6.0.0",
    "should": "^13.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "capitalize": "^2.0.0",
    "decamelize": "^5.0.0",
    "deepdash": "^5.3.0",
    "fast-deep-equal": "github:contra/fast-deep-equal",
    "graceful-fs": "^4.2.0",
    "human-schema": "^1.1.4",
    "inflection": "^1.12.0",
    "is-number": "^7.0.0",
    "is-plain-obj": "^3.0.0",
    "moment-timezone": "^0.5.27",
    "pg-query-stream": "^4.0.0",
    "pluralize": "^8.0.0",
    "pretty-ms": "^7.0.0"
  }
}
