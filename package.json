{
  "name": "iris-ql",
  "version": "0.0.2",
  "description": "User friendly API query language",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/iris-ql.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/iris-ql/issues"
  },
  "homepage": "https://github.com/staeco/iris-ql#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "babel src --copy-files --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test-make-user": "createuser postgres -lsd",
    "test-db": "dropdb iris-ql -U postgres || true && createdb iris-ql -U postgres && psql iris-ql -c 'CREATE EXTENSION IF NOT EXISTS postgis;' -U postgres",
    "test": "npm run-script test-db && mocha --require @babel/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint"
  },
  "peerDependencies": {
    "pg": ">=7",
    "sequelize": ">=5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@stae/babel-node": "^0.1.0",
    "@stae/linters": "^0.1.2",
    "eslint": "^5.0.0",
    "eslint-cli": "^1.0.0",
    "mocha": "^6.0.0",
    "pg": "^7.11.0",
    "pluralize": "^8.0.0",
    "rimraf": "^2.5.0",
    "sequelize": "^5.10.1",
    "should": "^13.0.0",
    "underscore": "^1.9.1"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "graceful-fs": "^4.2.0",
    "is-pure-object": "^1.0.1",
    "lodash": "^4.17.15"
  }
}
