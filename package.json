{
  "name": "iris-ql",
  "version": "2.0.1",
  "description": "User friendly API query language",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/iris-ql.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/iris-ql/issues"
  },
  "homepage": "https://github.com/staeco/iris-ql#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "NODE_ENV=production babel src --copy-files --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test --fix",
    "test-make-user": "createuser postgres -lsd",
    "test-db": "dropdb iris-ql -U postgres || true && createdb iris-ql -U postgres && psql iris-ql -c 'CREATE EXTENSION IF NOT EXISTS postgis;' -U postgres",
    "test": "npm run-script test-db && NODE_ENV=test nyc mocha --require @babel/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": true
  },
  "peerDependencies": {
    "pg": ">=7",
    "sequelize": ">=4"
  },
  "resolutions": {
    "pg-cursor": "github:contra/node-pg-cursor#patch-1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@stae/babel-node": "^0.1.0",
    "@stae/linters": "^0.1.2",
    "JSONStream": "^1.3.5",
    "babel-plugin-istanbul": "^5.2.0",
    "eslint": "^6.2.1",
    "eslint-cli": "^1.0.0",
    "get-stream": "^5.1.0",
    "mocha": "^6.0.0",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "pumpify": "^2.0.0",
    "rimraf": "^3.0.0",
    "sequelize": "^5.10.1",
    "should": "^13.0.0"
  },
  "dependencies": {
    "capitalize": "^2.0.0",
    "core-js": "3.0.0",
    "decamelize": "^3.2.0",
    "graceful-fs": "^4.2.0",
    "inflection": "^1.12.0",
    "is-number": "^7.0.0",
    "is-plain-object": "^3.0.0",
    "moment": "^2.24.0",
    "pg-cursor": "github:contra/node-pg-cursor#patch-1",
    "pg-query-stream": "github:contra/node-pg-query-stream",
    "pluralize": "^8.0.0",
    "pump": "^3.0.0",
    "through2": "^3.0.1"
  }
}
